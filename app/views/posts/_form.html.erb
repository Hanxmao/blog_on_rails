<div>
    <%= form_with(model: @post) do |f|%>
        <div class="mb-3">
            <%= f.label :title, class:"form-label"%><br>
            <%= f.text_field :title, class:"form-control" %>
        </div>
        <div class="mb-3">
            <%= f.label :body, class:"form-label"%>
            <%= f.text_area :body, class:"form-control", rows:5, cols:20 %>
        </div>
        <%= f.submit%>
    <% end %>
</div>



